{
    "_comment": "Demonstrates opt-in skip_empty_objects behavior: empty dict entries are skipped",
    "url": "/api/items",
    "match_subsets": true,
    "match_every_entry": true,
    "skip_empty_objects": true,
    "response": {
        "items": [
            {
                "score": "$gt 0"
            }
        ]
    }
}
